sense:R-ECM1-1
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "Checks if solar radiation is high or not" ;
  rdfs:label "high solar radiation" ;
  skos:definition "IF cooling season  is active AND solar radiation is high THEN lower blinds down and reduce cooling" ;
.
sense:R-ECM1-2
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "checks if some cooling is on while space unocuppied" ;
  rdfs:label "unused cooling rule" ;
  skos:definition "IF room occupancy = 0 AND room occupancy is unchanged for time T AND room cooling is on THEN shut room cooling off" ;
.
sense:R-ECM1-3
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "checks if some space is being undercooled" ;
  rdfs:label "undercooling" ;
  skos:definition "IF cooling season is active AND room temperature lowerThan Tmin THEN turn cooling down" ;
.
sense:R-ECM1-5
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "checks if some cooling energy is wasted through some opening" ;
  rdfs:label "wasted coolness through opening" ;
  skos:definition "IF cooling is on AND window is open THEN close window" ;
.
sense:R-ECM2-1
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "Checks if solar radiation is high or not" ;
  rdfs:label "high solar radiation" ;
  skos:definition "IF heating season  is active AND solar radiation is high THEN open room blinds up and reduce room heating" ;
.
sense:R-ECM2-2
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "checks if some heating is on while space unocuppied" ;
  rdfs:label "unused heating rule" ;
  skos:definition "IF room occupancy = 0 AND room occupancy is unchanged for time T AND room heating is on THEN shut room heating off" ;
.
sense:R-ECM2-2-1
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "checks if some heating is on while space unocuppied" ;
  rdfs:label "unused heating rule" ;
  skos:definition "IF heating season is active AND room occupancy = 0 AND room occupancy is unchanged for time T AND room temperature greaterThan Tref THEN shut room heating off" ;
.
sense:R-ECM2-3
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "checks if some space is being overheated" ;
  rdfs:label "space overheating" ;
  skos:definition "IF heating season is active AND room temperature greaterThan Tmax THEN turn heating down" ;
.
sense:R-ECM2-4-1
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "checks if air heating and air cooling work at the same time" ;
  rdfs:label "simultaneous air heating and cooling" ;
  skos:definition "IF air heating is on AND air cooling is on THEN air heating and air cooling work simultaneously" ;
.
sense:R-ECM2-4-2a
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "checks if the supply fan works but not the return/exhaust fan" ;
  rdfs:label "air supplied but not returned" ;
  skos:definition "IF supply air volume flow > 0 + tolerance AND return air volume flow < 0 + tolerance THEN air supplied but not returned" ;
.
sense:R-ECM2-4-2b
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "checks if the return/exhaust fan works but not the supply fan" ;
  rdfs:label "air returned but not supplied" ;
  skos:definition "IF return air flow > 0 + tolerance AND supply air flow < 0 + tolerance THEN air returned but not supplied" ;
.
sense:R-ECM2-4-3
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "checks if air heating works but not the supply fan" ;
  rdfs:label "air heated but not ventilated" ;
  skos:definition "IF air heating is on AND supply air flow < 0 + tolerance THEN air heated but not ventilated" ;
.
sense:R-ECM2-4-4
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "checks if air cooling works but not the supply fan" ;
  rdfs:label "air cooled but not ventilated" ;
  skos:definition "IF air cooling is on AND supply air flow < 0 + tolerance THEN air cooled but not ventilated" ;
.
sense:R-ECM2-5
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "checks if some heat is wasted through some opening" ;
  rdfs:label "wasted heat through opening" ;
  skos:definition "IF heating is on AND window is open THEN close window" ;
.
sense:R-ECM3-6
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "checks if some light was left on while space unocuppied" ;
  rdfs:label "unused lighting rule" ;
  skos:definition "IF room is unoccupied AND room light is on THEN shut room light off" ;
.
sense:R-ECM4-8
  a owl:NamedIndividual ;
  a sense:Function ;
  rdfs:comment "checks if some appliance is on while space unocuppied" ;
  rdfs:label "unused appliance rule" ;
  skos:definition "IF room is unoccupied AND room occupancy is unchanged for time T AND room appliance consumption increases THEN shut room appliance off" ;
.
