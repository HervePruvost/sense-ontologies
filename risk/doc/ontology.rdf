    <owl:Ontology rdf:about="https://www.w3id.org/risk">
        <terms:bibliographicCitation xml:lang="en">Pruvost. H. (2023): The RISK Ontology for a risk management paradigm in the Architecture, Engineering, Construction, and Facility Management domain. https://w3id.org/risk.</terms:bibliographicCitation>
        <terms:creator rdf:resource="https://orcid.org/0000-0001-7604-8543"/>
        <terms:creator rdf:resource="https://www.linkedin.com/in/hervepruvost"/>
        <terms:creator>Herv√© Pruvost</terms:creator>
        <terms:description xml:lang="en">The RISK ontology is a graph data structure that conceptualizes the domain of risk management by providing terms about risk identification, analysis and mitigation. It applies currently to the domain of engineering in the construction and facility management sector. But it aims at universally cover any domain.</terms:description>
        <terms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-06-22</terms:issued>
        <terms:license rdf:resource="http://creativecommons.org/licenses/by-sa/4.0"/>
        <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-06-07</terms:modified>
        <terms:publisher rdf:resource="https://www.eas.iis.fraunhofer.de/"/>
        <terms:publisher>Fraunhofer IIS/EAS</terms:publisher>
        <terms:title xml:lang="en">RISK: the risk ontology.</terms:title>
        <vann:preferredNamespacePrefix>risk</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://w3id.org/risk#</vann:preferredNamespaceUri>
        <rdfs:seeAlso rdf:resource="https://2023.eswc-conferences.org/wp-content/uploads/2023/05/paper_Pruvost_2023_Ontologies.pdf"/>
        <owl:versionInfo>v1.0.0</owl:versionInfo>
    </owl:Ontology>
    


