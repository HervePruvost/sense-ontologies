  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onDataRange xsd:string ;
      owl:onProperty risk:ID ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    ] ;
  owl:equivalentClass [
      a owl:Class ;
      owl:intersectionOf (
          risk:Problem
          [
            a owl:Restriction ;
            owl:onProperty risk:occursAt ;
            owl:someValuesFrom owl:Thing ;
          ]
        ) ;
    ] ;
