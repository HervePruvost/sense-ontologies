        <terms:crontributor>Hervé Pruvost</terms:crontributor>
        <terms:description xml:lang="en">Energy System Information Model (ESIM) is a domain specific model that provides information of the urban and building energy system including the automation and control systems. It comprises functional, structural and physical descriptions of the systems as master data and additional operational data.</terms:description>
        <terms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-09-30</terms:issued>
        <terms:license rdf:resource="http://creativecommons.org/licenses/by-sa/4.0"/>
        <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-06-17</terms:modified>
        <terms:publisher rdf:resource="https://www.eas.iis.fraunhofer.de/"/>
        <terms:publisher>Fraunhofer IIS/EAS</terms:publisher>
        <terms:title xml:lang="en">ESIM: Energy System Information Model.</terms:title>
        <vann:preferredNamespacePrefix>esim</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri>https://w3id.org/esim#</vann:preferredNamespaceUri>
        <spin:imports rdf:resource="http://topbraid.org/spin/owlrl-all"/>
